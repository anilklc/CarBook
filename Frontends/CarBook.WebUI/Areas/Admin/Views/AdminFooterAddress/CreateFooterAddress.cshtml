@model CreateFooterAddressDto
@{
    ViewData["Title"] = "CreateFooterAddress";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<div class="page-content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-xl-12">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">Create Footer Address</h4>
                        <p class="card-subtitle mb-4">Please fill in the information about the vehicle completely.</p>
                        <form method="post">
                            <div class="form-group">
                                <label for="exampleInputDescription">Description</label>
                                <input type="text" class="form-control" id="exampleInputDescription" asp-for="Description" placeholder="Description">
                            </div>
                            <div class="form-group">
                                <label for="exampleInputAddress">Address</label>
                                <input type="text" class="form-control" id="exampleInputAddress" asp-for="Address" placeholder="Address">
                            </div>
                            <div class="form-group">
                                <label for="exampleInputPhone">Phone</label>
                                <input type="tel" class="form-control" id="exampleInputPhone" asp-for="Phone" placeholder="Phone">
                            </div>
                            <div class="form-group">
                                <label for="exampleInputEmail">Email</label>
                                <input type="email" class="form-control" id="exampleInputEmail" asp-for="Email" placeholder="Email">
                            </div>

                            <button type="submit" class="btn btn-primary waves-effect waves-light">Create Footer Address</button>
                        </form>

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

