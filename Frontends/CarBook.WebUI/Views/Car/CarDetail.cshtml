
@{
    ViewData["Title"] = "CarDetail";
    Layout = "~/Views/UILayout/Index.cshtml";
}

<section class="ftco-section ftco-car-details">
	<div class="container">
		@await Component.InvokeAsync("_CarDetailMainCarFeatureComponentPartial", new { Id = ViewBag.Id })
		<div class="row">
			<div class="col-md-12 pills">
				<div class="bd-example bd-example-tabs">
					@await Component.InvokeAsync("_CarDetailTabPaneComponentPartial")
					<div class="tab-content" id="pills-tabContent">
						@await Component.InvokeAsync("_CarDetailCarFeatureByCarIdComponentPartial", new { Id = ViewBag.Id })
						@await Component.InvokeAsync("_CarDetailCarDescriptionByCarIdComponentPartial", new { Id = ViewBag.Id })
					</div>
				</div>
			</div>
		</div>
	</div>
</section>


